{
    "name": "Webcaddy PDM",
    "description": "Webcaddy Private Data Manager",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": "hello_extensions.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab","storage"],
    "host_permissions": ["https://developer.chrome.com/*"]    
}